#!/media/paul/TLinux/Github/wichit2s/csubu-linux-59/linux-cli/githubavatar/env/bin/python2
# EASY-INSTALL-ENTRY-SCRIPT: 'githubavatar==0.1.0','console_scripts','githubavatar'
__requires__ = 'githubavatar==0.1.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('githubavatar==0.1.0', 'console_scripts', 'githubavatar')()
    )
